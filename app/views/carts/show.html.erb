<% if notice %>
	<p id="notice">
	<%= notice %></p>
<% end %>
<h2>Your Aqua Heli Cart</h2>
<ul>
<% @cart.line_items.each do |item| %>
<li><%= item.quantity %> &times; <%= item.product.title %> <%= item.product.price %></li>
<% end %>

<%= link_to 'Edit', edit_cart_path(@cart) %> |
<%= link_to 'Back', carts_path %>
